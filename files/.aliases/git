git_concise_log_format='--pretty=format:%Cblue%h%Creset %cr %Cgreen%an%Creset %s'

alias s='git status'
alias ga='git add'
alias gs='git show'
alias d='git diff'
alias gd='git diff'
alias gdc='git diff --cached'
alias gl='git log -p'
alias glc='git log "$concise_logging_format"'
alias glcg='glc --graph'
